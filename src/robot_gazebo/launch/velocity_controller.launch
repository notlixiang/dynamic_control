<launch>

    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find robot_gazebo)/config/velocity_joint_controller.yaml" command="load"/>

    <node name="one_link_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/one_link" args="joint1_velocity_controller"/>

</launch>
